pragma solidity ^0.8;
pragma experimental ABIEncoderV2;


contract Cedro is Ownable {
    
    mapping(string=>bytes32) public registros;
    
    event registroAgregado(string _salt, bytes32 _hash);
    
    function setHash(string memory _salt,bytes32 _hash) public onlyOwner {
        require(keccak256(abi.encodePacked(_salt))!=keccak256(abi.encodePacked("")),"Salt vacio");
        require(_hash!="","Hash vacio");
        require(registros[_salt]=="","Salt ya seteado");
        registros[_salt]=_hash;
        emit registroAgregado(_salt,_hash);
    }
    
}
